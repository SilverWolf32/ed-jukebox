{
  "name": "ed-jukebox",
  "version": "0.0.1",
  "description": "A companion app for Elite: Dangerous that makes your favorite iTunes playlist responsive to in-game events.",
  "main": "main.js",
  "scripts": {
    "start": "zsh -f less.zsh; electron .",
    "start-win": "zsh -f less.zsh; node_modules/.bin/electron .",
    "install-wsl": "export npm_config_platform=win32 && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "elite-dangerous",
    "music",
    "itunes"
  ],
  "author": "seaturtle",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.8",
    "eventemitter3": "^3.1.0",
    "jquery": "^3.3.1",
    "less": "^3.8.0"
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "focus-visible": "^4.1.5"
  }
}
